<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/blue"
    android:orientation="vertical"
    android:padding="@dimen/default_margin"
    tools:context=".MainActivity">

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/score_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="נקודות: 0"
        android:textColor="@android:color/white"
        android:textSize="18sp"
        android:layout_gravity="end" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_margin="@dimen/default_margin">

        <androidx.appcompat.widget.AppCompatImageButton
            android:id="@+id/heart1"
            android:layout_width="@dimen/heart_dimens"
            android:layout_height="@dimen/heart_dimens"
            android:background="@android:color/transparent"
            android:clickable="false"
            android:contentDescription="@string/heart1_desc"
            android:focusable="false"
            android:src="@drawable/heart" />


        <androidx.appcompat.widget.AppCompatImageButton
            android:id="@+id/heart2"
            android:layout_width="@dimen/heart_dimens"
            android:layout_height="@dimen/heart_dimens"
            android:layout_marginHorizontal="@dimen/heart_margin"
            android:background="@android:color/transparent"
            android:clickable="false"
            android:contentDescription="@string/heart2_desc"
            android:focusable="false"
            android:src="@drawable/heart" />

        <androidx.appcompat.widget.AppCompatImageButton
            android:id="@+id/heart3"
            android:layout_width="@dimen/heart_dimens"
            android:layout_height="@dimen/heart_dimens"
            android:background="@android:color/transparent"
            android:clickable="false"
            android:contentDescription="@string/heart3_desc"
            android:focusable="false"
            android:src="@drawable/heart"
            android:visibility="visible" />

    </LinearLayout>



